{
  "name": "agent-rdp",
  "version": "0.1.1",
  "description": "CLI tool for AI agents to control Windows Remote Desktop sessions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thisnick/agent-rdp.git"
  },
  "license": "MIT OR Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "agent-rdp": "./bin/agent-rdp"
  },
  "files": [
    "bin",
    "scripts",
    "dist"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.cjs",
    "build:ts": "tsc",
    "build": "cargo build --release && node scripts/copy-native.cjs",
    "build:all": "node scripts/build-all.cjs",
    "prepublishOnly": "npm run build:ts",
    "example": "tsx examples/hello-world.ts"
  },
  "keywords": [
    "rdp",
    "remote-desktop",
    "windows",
    "automation",
    "cli",
    "agent"
  ],
  "bugs": {
    "url": "https://github.com/thisnick/agent-rdp/issues"
  },
  "homepage": "https://github.com/thisnick/agent-rdp#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
